
<!-- ====================================
——— PAGE TITLE SECTION
===================================== -->
<section class="pageTitleSection" id="">
  <div class="container">
    <div class="pageTitleInfo">
      <h2>{{# each pageOnas}}{{{title}}}{{/each}}</h2>
      <ol class="breadcrumb">
        <li><a href="/">Domů</a></li>
        <li class="active">{{# each pageOnas}}{{{title}}}{{/each}}</li>
      </ol>
    </div>
  </div>
</section>

<!-- ====================================
——— MAIN SECTION
===================================== -->
<section class="mainContent full-width clearfix aboutSection">
  <div class="container">
    <div class="row">
      <div class="col-sm-6 col-sm-push-6 col-xs-12">
        {{# each pageOnas}}
          {{#if image}}
            <img src="{{{cloudinaryUrl image crop="fit" height="450" }}}" alt="image" class="img-responsive img-rounded">
          {{/if}}
        {{/each}}
      </div>
      <div class="col-sm-5 col-sm-pull-5 col-xs-12">
        <div class="schoolInfo">
          <h2>MUDr. Helena Filová</h2>
            <p>{{# each pageOnas}}
                {{{description}}}
              {{/each}}
            </p>
